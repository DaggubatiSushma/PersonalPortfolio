<%- include ('partials/head') %> 
<body>

  <%- include ('partials/header') %>


  <main id="main">

<section id="projects" class="projects section-bg">
    <div class="container">

      <div class="section-title">
        <h2>Projects</h2>
         </div>

      <div class="row">
        <div class="col-lg-6">
            <h3 class="projects-title"><%=projects.title%></h3>
          <div class="projects-item">
            <h4><%=projects.occupation%></h4>
            <h5><%=projects.period%></h5>
            <h5><em><%=projects.company%></em></h5>
            <p>
            <ul>
              <li><%=projects.point1%></li>
              <li><%=projects.point2%></li>
              <li><%=projects.point3%></li>
              <li><%=projects.point4%></li>
            </ul>
            </p>
          </div>
          <div class="projects-item">
            <img src="images/project1.jpg" class="img-fluid" alt="">
            </div>
        </div>
        <div class="col-lg-6">
          <h3 class="projects-title"></h3>
          <div class="projects-item">
            <h4><%=projects.project2%></h4>
            <h5><%=projects.period2%></h5>
            <p>
            <ul>
              <li> <%=projects.point5%></li>
              <li><%=projects.point6%></li>
              <li> <%=projects.point7%></li>
              <li><%=projects.point8%></li>
            </ul>
            </p>
          </div>
          <div class="projects-item">
            <img src="images/project2.jpg" class="img-fluid" alt="">
            </div>
        </div>
      </div>

    </div>
  </section>

  
</main>


</body>
<%- include ('partials/footer') %>
<%- include ('partials/scripts') %>